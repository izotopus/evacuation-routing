    "@types/node": "^24.10.1",
    "dotenv": "^17.2.3",
    "eslint": "^9.39.1",
    "express": "^5.2.1",
    "geojson": "^0.5.0",
    "geojson-rbush": "^3.2.0",
    "install": "^0.13.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "npm": "^11.6.4",
    "prettier": "^3.7.4",
